    "#text to speech\n",
    "import gtts\n",
    "import playsound\n",
    "text = input(\"Enter something here: \")\n",
    "sound = gtts.gTTS(text, lang=\"en\")\n",
    "sound.save(\"welcome.mp3\")\n",
    "playsound.playsound(\"welcome.mp3\")"
   ]
  }
